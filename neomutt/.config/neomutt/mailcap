text/html; lynx -vikeys -nocolor -assume_charset=%{charset} -display_charset=utf-8 %s; nametemplate=%s.html
text/html; lynx -vikeys -nocolor -assume_charset=%{charset} -display_charset=utf-8 -dump %s; nametemplate=%s.html; copiousoutput
application/pdf; wpspdf %s; test=test -n "$DISPLAY"
image/*; sxiv %s; test=test -n "$DISPLAY"
